<%= form_for @driver do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :password%>
    <%= f.password_field :password%>

    <%= f.label :car %>
    <%= f.text_field :car %><br>

    <%= f.label :racemaster%>
    <%= f.text_field :racemaster%>

    <%=f.submit %>

<% end %>